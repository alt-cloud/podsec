[Unit]
Description=Usernetes kube-scheduler service
BindsTo=rootlesskit.service
Requires=kube-apiserver.service
After=kube-apiserver.service
PartOf=u7s-master.target

[Service]
ExecStart=/var/lib/u7s-admin/usernetes/bin/_kube-scheduler.sh
WorkingDirectory=/var/lib/u7s-admin/usernetes
  EnvironmentFile=/var/lib/u7s-admin/.config/usernetes/env
  Restart=on-failure
  LimitNOFILE=65536

